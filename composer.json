{
    "name": "faisalrizal/lumen-modules",
    "description": "Lumen 5.6 Modules",
    "keywords": ["modules", "lumen", "jasahub", "jasahub"],
    "license": "MIT",
    "authors": [
        {
            "name": "Faisal Rizal",
            "email": "faisalrizal@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "league/fractal": "0.17.0"
    },
    "autoload": {
        "psr-4": {
            "Jasahub\\Modules\\": "src/"
        }
    },
    "suggest": {
        "wikimedia/composer-merge-plugin": "Allows the ability to create and merge composer.json files for your individual modules for module-specific dependency management."
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
